image/gif; /usr/bin/imv %s
image/GIF; /usr/bin/imv %s
image/png; /usr/bin/imv %s
image/PNG; /usr/bin/imv %s
image/jpg; /usr/bin/imv %s
image/JPG; /usr/bin/imv %s
image/jpeg; /usr/bin/imv %s
image/JPEG; /usr/bin/imv %s
